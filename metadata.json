{
  "name": "simp-pupmod",
  "version": "7.0.1",
  "author": "SIMP Team",
  "summary": "A puppet module to manage puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-pupmod",
  "project_page": "https://github.com/simp/pupmod-simp-pupmod",
