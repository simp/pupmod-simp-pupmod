{
  "name": "simp-pupmod",
  "version": "8.0.0",
  "author": "SIMP Team",
  "summary": "A puppet module to manage puppetserver and puppet agents",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-pupmod",
  "project_page": "https://github.com/simp/pupmod-simp-pupmod",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "ci",
    "pupmod",
    "puppet",
    "puppetserver",
    "puppet-agent",
    "agent"
  ],
  "dependencies": [
    {
      "name": "simp/haveged",
      "version_requirement": ">= 0.4.5 < 1.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.19.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 2.5.0 < 4.0.0"
    },
    {
      "name": "puppetlabs/puppet_authorization",
      "version_requirement": ">= 0.2.0 < 1.0.0"
    },
    {
      "name": "puppetlabs/hocon",
      "version_requirement": ">= 0.9.3 < 2.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 3.0.0 < 7.0.0"
    },
    {
      "name": "simp/iptables",
      "version_requirement": ">= 6.1.2 < 7.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.14.1 < 5.0.0"
    },
    {
      "name": "simp/pki",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
      {
        "name": "camptocamp/systemd",
        "version_requirement": ">= 2.7.0 < 3.0.0"
      }
    ]
  },
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    }
  ]
}
